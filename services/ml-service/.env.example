# Environment Configuration
NODE_ENV=development

# Service Ports
ML_SERVICE_PORT=4000
MCP_SERVICE_PORT=4001
MCP_GRPC_PORT=50051

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/parkgolf-ml
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# NATS Configuration
NATS_SERVERS=nats://localhost:4222
SERVICE_NAME=parkgolf-ml-mcp

# Logging Configuration
LOG_LEVEL=debug
LOG_DIR=logs

# API Keys (for future ML services)
OPENAI_API_KEY=
GOOGLE_MAPS_API_KEY=
WEATHER_API_KEY=

# Security
JWT_SECRET=your-super-secret-jwt-key-here
API_RATE_LIMIT=100

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000

# Docker Configuration
COMPOSE_PROJECT_NAME=parkgolf-ml-mcp